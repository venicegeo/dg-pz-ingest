@Library('pipelib@master') _


node {
  def mvn = tool 'M3'
  def root = pwd()

 stage('Build/Install Job Common') {
    git([
      url: env.GIT_URL ? env.GIT_URL : 'https://github.com/venicegeo/dg-pz-jobcommon',
      branch: "master"
    ])
    sh """
      ${mvn}/bin/mvn install -U 
    """
  } 

  stage('Archive') {
    git([
      url: env.GIT_URL ? env.GIT_URL : 'https://github.com/venicegeo/dg-pz-ingest',
      branch: "master"
    ])
    sh """
      ${mvn}/bin/mvn clean package -U 
      cp target/piazza-ingest-1.0.0.jar ${root}/pz-ingest.jar
    """
  }

/*  stage('CI Deploy') {
    sh """
      cp target/piazza-ingest-1.0.0.jar ${root}/pz-ingest.jar
    """
    cfPush
  }

   
  stage('Staging Deploy') {
    cfPush {
      cfDomain  = 'apps.sys.coastline.dg-cf-test.net'
      cfSpace   = 'stage'
      ldapCredentialId = 'pcf_user'
    }
  } */

}
